{
  "extends": ["eslint-config-react-app"],
  "plugins": ["react-hooks", "@babel/preset-typescript"],
  "env": {
    "jest": true,
    "browser": true
  },
  "rules": {
    "react/prop-types": 0,
    "key-spacing": 0,
    "max-len": [2, 120, 2],
    "object-curly-spacing": [2, "always"],
    "semi": [2, "always"],
    "space-before-function-parent": 0,
    "react-hooks/rules-of-hooks": "warn",
    "react-hooks/exhaustive-deps": 0
  }
}
